#include<stdio.h>

int main(){
	int a, b, c;
	scanf("%d%d%d", &a, &b, &c);
	
	if((a+b>c) && (a+c>b) && (c+b>a)){
		if((a==b) && (b==c)){
			printf("Equilatero");
		}else if((a==b)||(b==c)||(c==a)){
			printf("Isosceles");
		}else{
			printf("Escaleno");
		}
	}else{
		printf("Nao existe");
	}
	
return 0;
}
